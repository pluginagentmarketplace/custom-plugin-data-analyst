{
  "name": "data-analyst-roadmap",
  "version": "2.0.0",
  "type": "plugin",
  "displayName": "Data Analyst Roadmap",
  "description": "Production-grade Data Analyst learning platform with 8 specialized agents, 12 skills, hands-on projects, and career guidance. Master Excel, SQL, Python, Statistics, Power BI, Machine Learning, and advance your analytics career from beginner to senior level.",
  "author": "Claude Developer Learning Community",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/pluginagentmarketplace/custom-plugin-data-analyst",
  "keywords": [
    "data-analyst",
    "data-analysis",
    "learning",
    "career-growth",
    "sql",
    "python",
    "power-bi",
    "statistics",
    "visualization",
    "spreadsheet",
    "analytics",
    "machine-learning",
    "data-storytelling"
  ],
  "agents": [
    "./agents/01-data-analytics-foundations.md",
    "./agents/02-sql-databases-expert.md",
    "./agents/03-statistical-analysis-expert.md",
    "./agents/04-visualization-architect.md",
    "./agents/05-programming-expert.md",
    "./agents/06-advanced-analytics-specialist.md",
    "./agents/07-career-coach.md",
    "./agents/08-data-storytelling.md"
  ],
  "skills": [
    "./skills/foundations/SKILL.md",
    "./skills/databases-sql/SKILL.md",
    "./skills/statistics/SKILL.md",
    "./skills/visualization/SKILL.md",
    "./skills/programming/SKILL.md",
    "./skills/advanced/SKILL.md",
    "./skills/career/SKILL.md",
    "./skills/excel/SKILL.md",
    "./skills/python-analytics/SKILL.md",
    "./skills/data-cleaning/SKILL.md",
    "./skills/business-intelligence/SKILL.md",
    "./skills/reporting/SKILL.md"
  ],
  "commands": [
    "./commands/explore.md",
    "./commands/learn.md",
    "./commands/progress.md",
    "./commands/projects.md"
  ],
  "configuration": {
    "autoLoadSkills": true,
    "enableProgressTracking": true,
    "enableProjects": true,
    "enableCareerGuidance": true,
    "observability": {
      "logging": true,
      "metrics": true,
      "tracing": true
    },
    "errorHandling": {
      "maxRetries": 3,
      "backoffStrategy": "exponential",
      "fallbackBehavior": "graceful_degradation"
    }
  },
  "compatibility": {
    "minClaudeCodeVersion": "0.5.0"
  },
  "qualityStandards": {
    "ethical": true,
    "honest": true,
    "transparent": true,
    "maintainable": true
  },
  "tags": [
    "data-analyst",
    "analytics",
    "data-science",
    "learning",
    "career-growth",
    "sql",
    "python",
    "visualization",
    "production-grade",
    "sasmp-2.0"
  ]
}